<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <AssemblyName>de.brickmakers.SecurityEngineering.AspSecurityHeaders</AssemblyName>
        <RootNamespace>de.brickmakers.SecurityEngineering.AspSecurityHeaders</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>de.brickmakers.SecurityEngineering.AspSecurityHeaders</PackageId>
        <Title>AspSecurityHeaders</Title>
        <Authors>BRICKMAKERS Security Engineering Team</Authors>
        <Copyright>Brickmakers GmbH</Copyright>
