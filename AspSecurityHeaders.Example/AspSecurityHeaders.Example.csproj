<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <Nullable>enable</Nullable>
        <TargetFrameworks>net5.0;net6.0;netcoreapp3.1</TargetFrameworks>
        <LangVersion>default</LangVersion>
        <RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>
        <DisableImplicitNuGetFallbackFolder>true</DisableImplicitNuGetFallbackFolder>
        <OpenApiGenerateDocumentsOnBuild>false</OpenApiGenerateDocumentsOnBuild>
        <AssemblyName>Brickmakers.AspSecurityHeaders.Example</AssemblyName>
        <RootNamespace>Brickmakers.AspSecurityHeaders.Example</RootNamespace>
        <PackageId>Brickmakers.AspSecurityHeaders.Example</PackageId>
        <Authors>Brickmakers.AspSecurityHeaders.Example</Authors>
        <Company>Brickmakers.AspSecurityHeaders.Example</Company>
        <Product>Brickmakers.AspSecurityHeaders.Example</Product>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\AspSecurityHeaders\AspSecurityHeaders.csproj"/>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.2.3"/>
    </ItemGroup>

</Project>
